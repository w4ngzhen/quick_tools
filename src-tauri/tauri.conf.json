{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "quick_tools",
  "version": "0.1.0",
  "identifier": "wang.zhen.app.quick-tools",
  "build": {
    "beforeDevCommand": "pnpm fe:dev",
    "devUrl": "http://localhost:8080",
    "beforeBuildCommand": "pnpm fe:build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "quick_tools",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
